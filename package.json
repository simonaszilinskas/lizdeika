  "version": "1.0.0",
  "description": "Frontend testing and modernization",
  "devDependencies": {
    "jest": "^29.0.0",
    "jsdom": "^22.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/user-event": "^14.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "puppeteer": "^21.0.0",
    "resemblejs": "^4.1.0",
    "benchmark": "^2.1.4",
