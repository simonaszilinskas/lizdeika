    "bcryptjs": "^2.4.3",
    "chromadb": "^3.0.17",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-correlation-id": "^3.0.1",
    "express-rate-limit": "^7.5.1",
