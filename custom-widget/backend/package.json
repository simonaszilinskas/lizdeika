    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
